           {user.data && <ProfilePopup user={user.data}/>}\r\n                </div>\r\n                <Button onClick={handleClick} variant=\"contained\" color=\"secondary\" className=\"ml-30\">\r\n                    Написать\r\n                </Button>\r\n            </div>\r\n\r\n\r\n            <AuthDialog  onClose={() => { setIsVisible(false) }} open={isVisible} />\r\n        </header>\r\n    );\r\n};\r\n"]},"metadata":{},"sourceType":"module"}